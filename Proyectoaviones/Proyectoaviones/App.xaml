<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Proyectoaviones.App">
    <Application.Resources>
        <Color x:Key="EvansGrey">#E0E0E0</Color>
        <Color x:Key="EvansOrange">#FF6600</Color>
        <Color x:Key="EvansWhite">#FFFFFF</Color>
        <Color x:Key="LabelOrange">#FF7139</Color>
        <Style x:Key="BarraEstadoS" TargetType="StackLayout">
            <Setter Property="BackgroundColor" Value="{StaticResource EvansGrey}" />
            <Setter Property="HorizontalOptions" Value="FillAndExpand" />
            <Setter Property="VerticalOptions" Value="FillAndExpand" />
        </Style>
    </Application.Resources>
</Application>